<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DU_RemainingTermMonthsPlanned__c</fullName>
    <deprecated>false</deprecated>
    <description>CDM: Demonstrates the planned remaining term in months</description>
    <externalId>false</externalId>
    <formula>CEILING((DU_DateEndPlanned__c - TODAY()) / 365 * 12)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Demonstrates the planned remaining term in months</inlineHelpText>
    <label>Remaining term in months</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
